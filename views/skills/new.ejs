<%- include('../partials/header.ejs', {title})%>

    <form action="/skills" method="POST" autocomplete="off">
        <div class="form-group">
            <label for="newSkillFrom">New Skill:</label>
            <input type="text" name="skill" class="form-control" id="newSkillFrom">
        </div>
        <div class="form-group">
            <label for="learnedAtForm">Learned At:</label>
            <input type="text" name="learnedAt" class="form-control" id="learnedAtForm" value="General Assembly">
        </div>
        <div class="form-group">
            <label for="furtherInfoForm">Further Info:</label>
            <input type="text" name="furtherInfo" class="form-control" id="furtherInfoForm">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

<%- include('../partials/footer.ejs')%>